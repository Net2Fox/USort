<Window x:Class="USort.Updater"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:USort"
        mc:Ignorable="d"
        Title="{DynamicResource l_NameDown}" Height="194.35" Width="442.977" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <ProgressBar x:Name="ProgressBar" HorizontalAlignment="Center" Height="29" Margin="63,89,62,0" VerticalAlignment="Top" Width="312"/>
        <TextBlock x:Name="TextDownload" HorizontalAlignment="Center" Margin="133,10,136,0" TextWrapping="Wrap" Text="{DynamicResource l_Download}" VerticalAlignment="Top" FontSize="16" Width="168" Height="26" TextAlignment="Center"/>
        <TextBlock x:Name="TextProgress" HorizontalAlignment="Center" Margin="133,41,136,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" FontSize="16" Width="168" Height="26" TextAlignment="Center"/>
    </Grid>
</Window>
