<Window x:Class="USort.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:USort"
        mc:Ignorable="d"
        Title="{DynamicResource l_About}" Height="194.432" Width="323.582" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" SizeToContent="Width">
    <Grid>
        <Label x:Name="Author_Label" Content="{DynamicResource l_Developer}" HorizontalAlignment="Center" Margin="74,6,74,0" VerticalAlignment="Top" FontSize="16"/>
        <Label x:Name="Versin2_Label" Content="{DynamicResource l_Version}" HorizontalAlignment="Center" Margin="124,82,124,0" VerticalAlignment="Top" FontSize="16"/>
        <Button x:Name="GitHub_Button" Content="{DynamicResource l_GitRepo}" HorizontalAlignment="Center" Margin="34,124,33,0" VerticalAlignment="Top" Width="251" Click="GitHub_Button_Click" FontSize="16"/>
        <Label x:Name="Thanks_Label" Content="{DynamicResource l_Thanks}" HorizontalAlignment="Center" Margin="57,43,56,0" VerticalAlignment="Top" FontSize="16" Width="205"/>

    </Grid>
</Window>
